page{
  box-sizing: border-box;
  padding:0 30rpx;
  padding-top: 30rpx !important;
}
#a2{
  background-color: red;
  font-size: 150px;
  display: none;
}
text{
  font-size: 38rpx;
  font-weight: 600;
  letter-spacing: 3rpx;
  display: block;
  margin: 40rpx 0 40rpx 20rpx;
  }
.big_box{
  border-radius: 20rpx;
  background-color:#fff; 
  padding: 25rpx 0 20rpx 30rpx;
  box-shadow:#cccc 4rpx 6rpx 20rpx;
  margin-bottom: 20rpx;

}
.big_box .title{
   font-size: 38rpx;
   letter-spacing: 3rpx;
   font-weight: 600;
   margin-bottom: 10rpx;
}
.big_box .num{
  margin-bottom: 25rpx;
  font-size: 26rpx;
}

.big_box .dian{
  margin-top: 8rpx;
  float: right;
  padding-bottom: 16rpx;
  width: 206rpx;
  font-size: 30rpx;
  text-align: center;
  border-left: 1px solid #cccccc;
  padding-top: 16rpx;
  box-sizing: border-box;
}
.big_box .dian .djfk{
  font-size: 30rpx;
  font-weight: 500;
  letter-spacing: 2rpx;
}
.big_box .dian .bg{ 
 height: 94rpx;
 width: 120rpx;
 background-size: 84rpx;
 background-repeat: no-repeat;
 margin-left: 65rpx;
 margin-bottom: 10rpx;
}
.big_box .name{
  font-size: 28rpx;
  font-weight: 500;
  background-position-y: center;
  background-repeat: no-repeat;
  background-size: 34rpx 34rpx;
  padding-left: 40rpx;
  background-image: url("data:image/png;base64,");
}
.time view{
  display: inline-block;
